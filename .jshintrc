{
	"browser": true,
	"node": true,

	"predef": [
        "define",
        "module",
        "before",
        "after",
        "beforeEach",
        "afterEach",
        "describe",
        "it"
    ],

	"boss": true,
	"curly": true,
	"eqnull": true,
	"expr": true,
	"globalstrict": false,
	"laxbreak": true,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonew": true,
	"quotmark": "single",
	"strict": false,
	"trailing": true,
	"undef": true,
	"unused": true,
	"laxcomma": true,

	"maxdepth": 4,
	"maxcomplexity": 10
}