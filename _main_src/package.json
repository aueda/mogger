{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js",
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "bootstrap": {
      "exports": "bootstrap",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "~0.7.0",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-concat": "^2.4.0",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "add-keyup-events": "0.0.2",
    "ampersand-collection": "^1.3.15",
    "ampersand-model": "^4.0.2",
    "ampersand-router": "^1.0.1",
    "ampersand-view": "^6.0.3",
    "ampersand-view-switcher": "^1.0.2",
    "bootstrap-sass": "^3.2.0",
    "devtools-detect": "git://github.com/saitodisse/devtools-detect",
    "highlight.js": "^8.2.0",
    "jquery": "~2.1.0",
    "lodash": "^2.4.1",
    "mogger": "^0.1.4"
  }
}
